<section id="business">
    <div class="container">
        <h1 class="mt-4">Бизнес</h1>
        <div class="row">
            <% for (const item of businessNew) { %>
            <div class="col-6 mb-3">
                <div class="business-box d-flex">
                    <!-- <img width="70%" height="auto" src="./images/1.png" alt=""> -->
                    <img width="50%" height="auto"  src="/uploads/news/<%= item.image %>" name="photo">
                    <div class="ml-3">
                        <div class="small-info d-flex">
                            <p class="mb-0"> <span><img src="./icons/Calindar.png" alt=""></span> 14.09.2020</p>
                            <p class="mb-0"> <span><img src="./icons/Eye.png" alt=""></span> 268 473</p>
                        </div>
                        <p class="business-text mt-3"><%= item.title %></p>
                    </div>

                </div>
            </div>
            <% } %>
        </div>
    </div>
</section>

