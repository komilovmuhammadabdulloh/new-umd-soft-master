<!-- MAIN SECTION -->
<section id="news">

    <div class="container">

        <div class="row mt-4">

            <div class="col-lg-6 col-sm-12">
                <h2>Сунгги янгиликлар</h2>
                <% for (const item of lastNew) { %>
                <div class="card">
                   
                        <div class="card-header">
                            <!-- <img class="w-100" src="./images/money.jpg" alt=""> -->
                            <img class="w-100" src="/uploads/news/<%= item.image %>" name="photo">
                        </div>
                        <div class="card-body">
                            <div class="small-info d-flex mt-2">
                                <p> <span><img src="./icons/Calindar.png" alt=""></span> 14.09.2020</p>
                                <p> <span><img src="./icons/Eye.png" alt=""></span> 268 473</p>
                            </div>
                            <h2><%= item.title %> </h2>
                            <p><%= item.description %></p>
                        </div>
                </div>
                <% } %>
            </div>

            <div class="col-lg-6 col-sm-12">

                <div class="main-news">
                    <h2>Асосий янгиликлар</h2>
                    <% for (const item of mainNew) { %>
                    <div class="news-box d-flex justify-content-around m">
                        <!-- <img class="w-100" src="./images/person.jpg" alt=""> -->
                        <img class="w-50" src="../uploads/news/<%= item.image %>" />
                        <div class="ml-3">
                            <div class="small-info d-flex">
                                <p class="mb-0"> <span><img src="./icons/Calindar.png" alt=""></span> 14.09.2020</p>
                                <p class="mb-0"> <span><img src="./icons/Eye.png" alt=""></span> 268 473</p>
                            </div>
                            <p class="main-news-text"><%= item.title %></p>
                        </div>
                    </div>
                    <% } %>
                    <hr>
                </div>
            </div>


        </div>
        <hr>
    </div>
</section>

<section id="jahon-news">
    <div class="container ">
        <h2 class="mt-3">Жахон янгиликлари</h2>
        <div class="row">
            <% for (const item of worldNew) { %>
            <div class="col-3">
                <div class="card ">
                   
                    <div class="card-header">
                        <!-- <img class="w-100" src="./images/amazon.png" alt=""> -->
                        <img class="w-50" src="../uploads/news/<%= item.image %>" />
                    </div>
                    <div class="card-body">
                        <div class="small-info d-flex mt-3 mb-2">
                            <p class="mb-0"> <span><img src="./icons/Calindar.png" alt=""></span> 14.09.2020</p>
                            <p class="mb-0"> <span><img src="./icons/Eye.png" alt=""></span> 268 473</p>
                        </div>
                        <p><%= item.title %></p>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
    </div>
</section>

<section id="contact">
    <div class="container">

        <!-- <div class="borderedd mt-4 mb-5"> -->
